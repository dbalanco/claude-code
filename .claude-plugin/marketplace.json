{
  "name": "nextjs-saas-marketplace",
  "owner": {
    "name": "Leon van Zyl",
    "email": "leon.vanzyl@gmail.com",
    "url": "https://github.com/leonvanzyl"
  },
  "metadata": {
    "description": "Comprehensive plugin marketplace for Next.js 15 SaaS developers with React 19, Tailwind, shadcn/ui, Postgres, Drizzle ORM, AI SDK, BetterAuth, and Polar",
    "version": "1.0.0"
  },
  "plugins": [
    {
      "name": "nextjs-core",
      "source": "./plugins/nextjs-core",
      "description": "Next.js 15 and React 19 development tools, commands, and agents",
      "version": "1.0.0",
      "category": "framework",
      "keywords": ["nextjs", "react", "app-router", "server-components"]
    },
    {
      "name": "database-tools",
      "source": "./plugins/database-tools",
      "description": "Drizzle ORM and Postgres database management tools",
      "version": "1.0.0",
      "category": "database",
      "keywords": ["drizzle", "postgres", "orm", "migrations"]
    },
    {
      "name": "auth-tools",
      "source": "./plugins/auth-tools",
      "description": "BetterAuth integration and authentication helpers",
      "version": "1.0.0",
      "category": "authentication",
      "keywords": ["betterauth", "auth", "authentication", "security"]
    },
    {
      "name": "payment-tools",
      "source": "./plugins/payment-tools",
      "description": "Polar payment processing integration",
      "version": "1.0.0",
      "category": "payments",
      "keywords": ["polar", "payments", "subscriptions", "billing"]
    },
    {
      "name": "ui-components",
      "source": "./plugins/ui-components",
      "description": "shadcn/ui and Tailwind CSS component generation and utilities",
      "version": "1.0.0",
      "category": "ui",
      "keywords": ["shadcn", "tailwind", "ui", "components", "design"]
    },
    {
      "name": "ai-integration",
      "source": "./plugins/ai-integration",
      "description": "Vercel AI SDK integration helpers and patterns",
      "version": "1.0.0",
      "category": "ai",
      "keywords": ["ai", "vercel-ai-sdk", "llm", "streaming"]
    },
    {
      "name": "testing-quality",
      "source": "./plugins/testing-quality",
      "description": "Testing, linting, and code quality tools for Next.js",
      "version": "1.0.0",
      "category": "testing",
      "keywords": ["testing", "vitest", "playwright", "eslint", "quality"]
    }
  ]
}
